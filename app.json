{
  "expo": {
    "name": "JokeXP",
    "slug": "JokeXP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#015AEF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "JokeXP"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#015AEF"
      },
      "package": "com.agenorhouessou.JokeXP"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7bd3860a-22f8-4254-9f75-f4a1ec7bdbe3"
      }
    }
  }
}
